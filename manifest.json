{
  "devMode": true,
  "experimental": false,
  "multiple": false,
  "background": false,
  "hiddenHandle": false,
  "forceUpdate": false,
  "openWith": "lastActive",
  "behavior": "application",
  "version": "0.1",
  "title": "CodeMirror",

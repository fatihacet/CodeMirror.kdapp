  "path": "~/Applications/CodeMirror.kdapp",
  "homepage": "devrim.localhost/codemirror",
  "author": "Devrim Yasar",
  "authorNick": "devrim",
  "repository": "git://github.com/devrim/codemirror.kdapp.git",
